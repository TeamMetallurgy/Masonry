{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "masonry:block/stonepaverspost"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "masonry:block/stonepaversside",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "low"
      },
      "apply": {
        "model": "masonry:block/stonepaversside",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "low"
      },
      "apply": {
        "model": "masonry:block/stonepaversside",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "low"
      },
      "apply": {
        "model": "masonry:block/stonepaversside",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "masonry:block/stonepaverssidetall",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "masonry:block/stonepaverssidetall",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "masonry:block/stonepaverssidetall",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "masonry:block/stonepaverssidetall",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
